{
	"name": "infiniteCascade/cascade-lib",
	"description": "Cascade CRM libraries",
	"keywords": ["yii", "crm"],
	"homepage": "http://www.infinitecascade.com",
	"type": "yii2-extension",
	"license": "GPL -2.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-imagine": "2.0.*",
		"InfiniteCascade/yii2-infinite-core": "*",
		"twitter/typeahead.js": "*",
		"phpoffice/phpexcel": "*",
		"emberlabs/gravatarlib": "*",
		"mistic100/jquery-querybuilder": "dev-master",
		"pragmaticly/smart-time-ago": "*",
		"bower-asset/vibe": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
		"yiisoft/yii2-jui": "*",
		"mustangostang/spyc": "*",
		"fabpot/php-cs-fixer": "@stable"
	},
    "autoload": {
		"classmap": ["library/cascade/Cascade.php"],
		"psr-0": { "cascade\\": "library" }
    },
    "extra": {
    	"bootstrap": "cascade\\Cascade"
    }
}
